{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": ["^build"]
    },
    "build:ci": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": ["^build:ci"]
    },
    "clear": {
      "dependsOn": ["^clear"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "format:fix": {
      "dependsOn": ["^format:fix"]
    },
    "storybook": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["lint", "^build"]
    }
  },
  "globalEnv": []
}
